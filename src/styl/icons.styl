@charset "UTF-8";

@font-face {
  font-family: "culturaviva";
  src:url("/fonts/culturaviva.eot");
  src:url("/fonts/culturaviva.eot?#iefix") format("embedded-opentype"),
    url("/fonts/culturaviva.woff") format("woff"),
    url("/fonts/culturaviva.ttf") format("truetype"),
    url("/fonts/culturaviva.svg#culturaviva") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "culturaviva" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "culturaviva" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

